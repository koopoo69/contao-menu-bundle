{
    "name": "heimrichhannot/contao-menu-bundle",
    "type": "contao-bundle",
    "description": "This bundle adds a multi-level menu module for the Contao CMS.",
    "keywords": [
        "contao",
        "menu"
    ],
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/heimrichhannot/contao-menu-bundle",
    "authors": [
        {
            "name": "Heimrich & Hannot",
            "email": "digitales@heimrich-hannot.de",
            "homepage": "https://www.heimrich-hannot.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimrichhannot/contao-menu-bundle/issues"
    },
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "4.4.*",
        "heimrichhannot/contao-utils-bundle": "^2.116"
    },
    "require-dev": {
        "contao/test-case": "1.1.*",
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": "^6.5",
        "phpunit/phpunit-mock-objects": "^4.0|^5.0",
        "phpunit/php-token-stream": "^1.4|^2.0|^3.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "satooshi/php-coveralls": "^2.0",
        "symfony/phpunit-bridge": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\MenuBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HeimrichHannot\\MenuBundle\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\MenuBundle\\ContaoManager\\Plugin",
        "foxy": true
    }
}
